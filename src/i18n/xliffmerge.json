        "targetPraefix": "",
        "targetSuffix": "",
        "beautifyOutput": false,
        "preserveOrder": true,
        "allowIdChange": false,
        "autotranslate": false,
        "apikey": "",
        "apikeyfile": "",
        "verbose": false,
        "quiet": false
    }
}