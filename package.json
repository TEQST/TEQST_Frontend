{
  "name": "teqst",
  "version": "0.0.1",
  "author": "Ionic Framework",
  "homepage": "https://ionicframework.com/",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "int:extract": "ng xi18n --output-path src/i18n",
    "build-i18n:de": "ng build --output-path=dist/de --aot --prod --i18n-file=src/i18n/messages.de.xlf --i18n-format=xlf --i18n-locale=de",
    "build-i18n:en": "ng build --output-path=dist/en --aot --prod --i18n-file=src/i18n/messages.en.xlf --i18n-format=xlf --i18n-locale=en",
    "build-i18n": "npm run build-i18n:en && npm run build-i18n:de",
    "dev": "lite-server --baseDir=www",
    "dev:de": "lite-server --baseDir=dist/de",
    "extract-translations": "ngx-translate-extract --input ./src --output ./src/assets/i18n/{en,ar,de,ja,zh_HANS,zh_HANT,es,fr,it,ru}.json --clean --sort --format json"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "~11.2.0",
    "@angular/core": "~11.2.0",
    "@angular/forms": "~11.2.0",
    "@angular/platform-browser": "~11.2.0",
    "@angular/platform-browser-dynamic": "~11.2.0",
    "@angular/router": "~11.2.0",
    "@angular/service-worker": "^11.2.0",
    "@ionic-native/core": "^5.30.0",
    "@ionic-native/splash-screen": "^5.30.0",
    "@ionic-native/status-bar": "^5.30.0",
    "@ionic/angular": "^5.5.2",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "cordova-browser": "6.0.0",
    "core-js": "^3.8.1",
    "file-saver": "^2.0.5",
    "hammerjs": "^2.0.8",
    "opus-recorder": "^8.0.3",
    "recordrtc": "^5.6.1",
    "rollbar": "^2.19.4",
    "rxjs": "^6.6.3",
    "streamsaver": "^2.0.5",
    "tslib": "^2.0.3",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^11.1.1",
    "@angular-devkit/architect": "^0.1000.8",
    "@angular-devkit/build-angular": "^0.1102.0",
    "@angular-devkit/core": "^10.0.8",
    "@angular-devkit/schematics": "^10.0.8",
    "@angular/cli": "^11.2.0",
    "@angular/compiler": "^11.2.0",
    "@angular/compiler-cli": "^11.2.0",
    "@angular/language-service": "^11.2.0",
    "@biesbjerg/ngx-translate-extract": "^7.0.3",
    "@ionic/angular-toolkit": "^2.3.3",
    "@types/jasmine": "^3.5.14",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "^14.0.27",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "cordova-plugin-device": "^2.0.3",
    "cordova-plugin-ionic-keyboard": "^2.2.0",
    "cordova-plugin-ionic-webview": "^5.0.0",
    "cordova-plugin-splashscreen": "^5.0.4",
    "cordova-plugin-statusbar": "^2.4.3",
    "cordova-plugin-whitelist": "^1.3.4",
    "eslint": "^7.15.0",
    "eslint-config-google": "^0.14.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.2",
    "karma": "^5.1.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~3.3.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "lite-server": "^2.6.1",
    "protractor": "^7.0.0",
    "ts-node": "~8.10.2",
    "tslint": "^6.1.3",
    "typescript": "~4.0.2"
  },
  "description": "An Ionic project",
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {},
      "cordova-plugin-statusbar": {},
      "cordova-plugin-device": {},
      "cordova-plugin-splashscreen": {},
      "cordova-plugin-ionic-webview": {},
      "cordova-plugin-ionic-keyboard": {}
    },
    "platforms": [
      "browser"
    ]
  }
}
